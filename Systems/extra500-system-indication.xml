		<fcs_function name="aircraft/engine/FF-lbs_h">
    			<function>
      			<description>fuel flow in pounds per hour</description>
			<product>
				<property>propulsion/engine/power-hp</property>   
				<sum>
					<product>
						<property>aircraft/engine/fuelflow-3rd</property>
						<property>propulsion/engine/power-hp</property>   
