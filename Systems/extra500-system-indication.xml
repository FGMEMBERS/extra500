		<fcs_function name="aircraft/engine/Delta-TOTsd-degC">
    			<function>
       			<description>TOT change when spooling down</description>
				<product>
					<property>sim-dt-sec</property>
					<value>0.00015</value>
					<pow>
						<max>
							<value>0.1</value>
							<property>propulsion/engine/n1</property>
						</max>
						<value>2</value>
