<PropertyList>

<sim>
	
	<description>Extra 500</description>
	<long-description>
		Stepping into an EA-500 is stepping into luxury. The impressively large six-seater cabin makes traveling 
		the ultimate experience in comfort, and an ergonomically arranged cockpit and state-of-the-art avionics 
		ensure that you have the best at your fingertips. 
		It’s also one of the smartest investments you’ll ever make. 
		Our engineers have found that perfect balance of power and weight to make the advanced carbon-fiber 
		EXTRA 500 the most cost effective single-engine turboprop aircraft available. 
	</long-description>

	<tags>
		<tag>Extra Aircraft</tag>
		<tag>turboprop</tag>
		<tag>single-engine</tag>
	</tags>

	<author>Thomas Grossberger, Dirk Dittmann</author>
	<status>alpha production 0.1.0</status>
	<!--
	<rating>
		<FDM type="int">0</FDM>
		<systems type="int">0</systems>
		<model type="int">0</model>
		<cockpit type="int">0</cockpit>
	</rating>
	-->

	<flight-model archive="y">jsb</flight-model>
	<aero archive="y">extra500</aero>
	
	<allow-toggle-cockpit type="bool">true</allow-toggle-cockpit>
	
	<model>
		<path archive="y">Aircraft/extra500/Models/extra500.xml</path>
	</model>

	<startup>
		<splash-texture>Aircraft/extra500/splash.png</splash-texture>
	</startup>

	<dimensions>
		<radius-m type="double">6</radius-m>
	</dimensions>
	<aircraft-class type="string">ga</aircraft-class>
	<aircraft-operator type="string">NONE</aircraft-operator>

	<help>
		<title>The Extra500 very special keys</title>
     	      <key>
			<name>Ctrl-F</name>
			<desc>Pitch control by pitch trim only</desc>
		</key>
	</help>
	
	<!-- position the pilot viewpoint and angle -->
	<view>
		<internal type="bool" archive="y">true</internal>
		<config>
			<x-offset-m archive="y" type="double">-0.21</x-offset-m>
			<y-offset-m archive="y" type="double">0.235</y-offset-m>
			<z-offset-m archive="y" type="double">0.36</z-offset-m>
			<pitch-offset-deg type="double">-12</pitch-offset-deg>
		</config>
	</view>
	<view n="101">
		<name>Model Front View</name>
		<enabled type="bool">true</enabled>
		<type>lookfrom</type>
		<internal type="bool">true</internal>
		<config>
			<from-model type="bool">true</from-model>
			<from-model-idx type="int">true</from-model-idx>
			<!-- x/y/z == right/up/back -->
			<x-offset-m archive="y"> 0.0</x-offset-m>
			<y-offset-m archive="y"> 0.0</y-offset-m>
			<z-offset-m archive="y">-10.0</z-offset-m>
			<pitch-offset-deg archive="y">0.0</pitch-offset-deg>
			<heading-offset-deg archive="y">180.0</heading-offset-deg>
		</config>
	</view>
	<view n="102">
		<name>Model Right View</name>
		<enabled type="bool">true</enabled>
		<type>lookfrom</type>
		<internal type="bool">true</internal>
		<config>
			<from-model type="bool">true</from-model>
			<from-model-idx type="int">true</from-model-idx>
			<!-- x/y/z == right/up/back -->
			<x-offset-m archive="y"> 10.0</x-offset-m>
			<y-offset-m archive="y"> 0.0</y-offset-m>
			<z-offset-m archive="y"> 0.0</z-offset-m>
			<pitch-offset-deg archive="y">0.0</pitch-offset-deg>
			<heading-offset-deg archive="y">90.0</heading-offset-deg>
		</config>
	</view>
	<view n="103">
		<name>Model Left View</name>
		<enabled type="bool">true</enabled>
		<type>lookfrom</type>
		<internal type="bool">true</internal>
		<config>
			<from-model type="bool">true</from-model>
			<from-model-idx type="int">true</from-model-idx>
			<!-- x/y/z == right/up/back -->
			<x-offset-m archive="y"> -10.0</x-offset-m>
			<y-offset-m archive="y"> 0.0</y-offset-m>
			<z-offset-m archive="y"> 0.0</z-offset-m>
			<pitch-offset-deg archive="y">0.0</pitch-offset-deg>
			<heading-offset-deg>-90.0</heading-offset-deg>
			<default-heading-deg type="double">-90.0</default-heading-deg>

		</config>
	</view>
	<view n="104">
		<name>Model Back View</name>
		<enabled type="bool">true</enabled>
		<type>lookfrom</type>
		<internal type="bool">true</internal>
		<config>
			<from-model type="bool">true</from-model>
			<from-model-idx type="int">true</from-model-idx>
			<!-- x/y/z == right/up/back -->
			<x-offset-m archive="y"> 0.0</x-offset-m>
			<y-offset-m archive="y"> 0.0</y-offset-m>
			<z-offset-m archive="y"> 10.0</z-offset-m>
			<pitch-offset-deg archive="y">0.0</pitch-offset-deg>
			<heading-offset-deg archive="y">0.0</heading-offset-deg>
		</config>
	</view>
	<view n="104">
		<name>Model Heli Back View</name>
		<enabled type="bool">true</enabled>
		<type>lookat</type>
		<config>
			<from-model type="bool">false</from-model>
			<from-model-idx type="int">0</from-model-idx>
			<eye-lat-deg-path>/position/latitude-deg</eye-lat-deg-path>
			<eye-lon-deg-path>/position/longitude-deg</eye-lon-deg-path>
			<eye-alt-ft-path>/position/altitude-ft</eye-alt-ft-path>
			<eye-heading-deg-path>/orientation/heading-deg</eye-heading-deg-path>

			<at-model type="bool">true</at-model>
			<at-model-idx type="int">0</at-model-idx>
			<at-model-heading-damping type="double">0.0</at-model-heading-damping>

			<target-x-offset-m type="double">0.0</target-x-offset-m>
			<target-y-offset-m type="double">1.4</target-y-offset-m>
			<target-z-offset-m type="double">5.8</target-z-offset-m>

			<ground-level-nearplane-m type="double">0.5f</ground-level-nearplane-m>
			<default-field-of-view-deg type="double">55.0</default-field-of-view-deg>

			<x-offset-m type="double">0.0</x-offset-m>
			<y-offset-m type="double">2.0</y-offset-m>
			<z-offset-m type="double">7.0</z-offset-m>
		</config>
	</view>
