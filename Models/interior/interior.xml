<!--pick animations-->
<animation>
  <type>pick</type>
  <object-name>storage.right.door</object-name>
  <visible>true</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script><![CDATA[
		UI.click("storagedoorright open/close");
	]]></script>
    </binding>
  </action>
</animation>

 <animation>
  <type>rotate</type>
  <object-name>storage.right.door</object-name>
  <property>/extra500/interior/storageright/doorstate</property>
  <interpolation>
   <entry><ind>0</ind><dep>0</dep></entry>
   <entry><ind>1</ind><dep>180</dep></entry>
  </interpolation>   
  <axis>
    <x1-m>0.16348</x1-m>
    <y1-m>0.14836</y1-m>
    <z1-m>-0.82936</z1-m>
    <x2-m>0.42076</x2-m>
    <y2-m>0.14836</y2-m>
    <z2-m>-0.82939</z2-m>
  </axis>
</animation>

