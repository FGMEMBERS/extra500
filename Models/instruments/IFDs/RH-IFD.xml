<PropertyList>

<path>RH-IFD.ac</path>

<!--Dimming Test -->
<animation>
        <type>material</type>
        <object-name>RH-IFD.Screen</object-name>
        <emission>
            <red>0.7</red>
            <green>0.7</green>
            <blue>0.7</blue>
            <factor-prop>extra500/instrumentation/IFD-RH/Backlight/state</factor-prop>
        </emission>
</animation>

<!-- dimming button -->
<animation>
      <type>pick</type>
      <object-name>Dim.up</object-name>
      <visible>false</visible>
      <action>
        <button>0</button>
        <repeatable>true</repeatable>
		<binding>
			<command>nasal</command>
			<script>
				<![CDATA[
				UI.click("IFD RH DIM >");
				]]>
			</script>
		</binding>
      </action>
</animation>

<animation>
      <type>pick</type>
