<PropertyList>

<path>MagneticCompass.ac</path>

<animation>
  <type>rotate</type>
	<object-name>Compassrose</object-name>
	<property>/instrumentation/magnetic-compass/indicated-heading-deg</property>
	<offset-deg>90</offset-deg>
	<factor>-1</factor>
	<axis>
		<x1-m>0</x1-m>
		<y1-m>0</y1-m>
		<z1-m>1</z1-m>
		<x2-m>0</x2-m>
		<y2-m>0</y2-m>
		<z2-m>-1</z2-m>
	</axis>
</animation>

</PropertyList>


