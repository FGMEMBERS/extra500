<PropertyList>

<path>FuelInstruments.ac</path>

<!--Left Collector Needle-->
<animation>
  <type>rotate</type>
  <object-name>FuelInstruments.CollLeft.Needle</object-name>
  <property>/consumables/fuel/tank[2]/level-norm</property>
		<interpolation>
            <entry>
                <ind>0</ind>
                <dep>-90.0</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>90.0</dep>
            </entry>
        </interpolation>
  <axis>
  <x1-m>1</x1-m>
  <y1-m>-0.02421</y1-m>
  <z1-m>0.05576</z1-m>
  <x2-m>-1</x2-m>
  <y2-m>-0.02421</y2-m>
  <z2-m>0.05576</z2-m>
  </axis>
</animation>
