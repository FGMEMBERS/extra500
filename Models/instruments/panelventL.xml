<PropertyList>

<path>panelvent.ac</path>

<animation>
  <type>rotate</type>
  <object-name>ball</object-name>
  <object-name>ball.inner</object-name>
  <property>/extra500/instrumentation/panelventswitchL/updown</property>
  <axis>
    <x1-m>-0.005709</x1-m>
    <y1-m>0</y1-m>
    <z1-m>0</z1-m>
    <x2-m>-0.005709</x2-m>
    <y2-m>1</y2-m>
    <z2-m>0</z2-m>
  </axis>
</animation>

<animation>
  <type>rotate</type>
  <object-name>ball</object-name>
  <object-name>ball.inner</object-name>
  <property>/extra500/instrumentation/panelventswitchL/leftright</property>
  <axis>
    <x1-m>-0.005709</x1-m>
    <y1-m>0</y1-m>
    <z1-m>0</z1-m>
    <x2-m>-0.005709</x2-m>
    <y2-m>0</y2-m>
    <z2-m>1</z2-m>
  </axis>
</animation>

<!--pick animations-->
<animation>
  <type>pick</type>
  <object-name>up</object-name>
  <visible>true</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script><![CDATA[
		UI.click("panelventL up");
	]]></script>
    </binding>
  </action>
</animation>

<animation>
  <type>pick</type>
  <object-name>down</object-name>
  <visible>true</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script><![CDATA[
		UI.click("panelventL down");
	]]></script>
    </binding>
  </action>
</animation>

<animation>
  <type>pick</type>
  <object-name>left</object-name>
  <visible>true</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script><![CDATA[
		UI.click("panelventL left");
	]]></script>
    </binding>
  </action>
</animation>

<animation>
  <type>pick</type>
  <object-name>right</object-name>
  <visible>true</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script><![CDATA[
		UI.click("panelventL right");
	]]></script>
    </binding>
  </action>
</animation>

</PropertyList>

