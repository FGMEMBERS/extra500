<PropertyList>

<path>TEST.fahrwerk.komplett.ac</path>

<animation>
  <name>BugFahrwerkKomplett</name>
  <object-name>Bugrad_Daempfer</object-name>
  <object-name>BugradGabel</object-name>
  <object-name>Bugrad</object-name>
  <object-name>BugradGelenkUnten</object-name>
  <object-name>BugradGelenkOben</object-name>
 </animation>
 
<animation>
  <name>LinksFahrwerkKomplett</name>
  <object-name>Radlinks</object-name>
  <object-name>RadStangelinks</object-name>
  <object-name>RadStangelinks.001</object-name>
</animation>
  
<animation>
  <name>RechtsFahrwerkKomplett</name>
  <object-name>Radrechts</object-name>
  <object-name>RadStangerechts</object-name>
  <object-name>RadStangerechts.001</object-name>
</animation> 

<!-- BUGFAHRWERK --> 

<!-- Bugradeinzug  -->
<animation>
  <type>rotate</type>
  <object-name>BugFahrwerkKomplett</object-name>
<property>gear/gear[0]/position-norm</property>
  <interpolation>
            <entry>
                <ind>0.0</ind>
                <dep>105</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>0</dep>
            </entry>
        </interpolation>
  <axis>
    <x1-m>-1.60481</x1-m>
    <y1-m>1.0</y1-m>
    <z1-m>-0.98046</z1-m>
	<x2-m>-1.60481</x2-m>
    <y2-m>-1.0</y2-m>
    <z2-m>-0.98046</z2-m>
  </axis>
  </animation> 
  
<!-- lenkung Bugfahrwerk-->  
<animation>
  <type>rotate</type>
  <object-name>BugFahrwerkKomplett</object-name>
  <condition>
    <equals>
      <property>gear/gear[0]/position-norm</property>
      <value>1</value>
    </equals>
  </condition>
  <property>surface-positions/rudder-pos-norm</property>
  <interpolation>
   <entry><ind>-1</ind><dep>-30</dep></entry>
   <entry><ind> 0</ind><dep>  0</dep></entry>
   <entry><ind> 1</ind><dep> 30</dep></entry>
  </interpolation>   
  <axis>
    <x1-m>-1.63609</x1-m>
    <y1-m>0</y1-m>
    <z1-m>-1.14638</z1-m>
    <x2-m>-1.69078</x2-m>
    <y2-m>0</y2-m>
    <z2-m>-1.43637</z2-m>
  </axis>
</animation>
  
<!-- Bugfahrwerksdaempfung -->
<animation>
  <type>translate</type>
  <object-name>BugradGabel</object-name>
  <object-name>BugradGelenkUnten</object-name>
  <object-name>Bugrad</object-name>
<property>gear/gear/compression-norm</property>
<interpolation>
            <entry>
                <ind>0.73</ind>
                <dep>-0.12</dep>
            </entry>
            <entry>
                <ind>0.3</ind>
                <dep>0.0</dep>
            </entry>
        </interpolation>
  <axis>
  <x1-m>-1.63609</x1-m>
  <y1-m>0</y1-m>
  <z1-m>-1.14638</z1-m>
  <x2-m>-1.69078</x2-m>
  <y2-m>0</y2-m>
  <z2-m>-1.43637</z2-m>
  </axis>
</animation> 
<animation>
  <type>rotate</type>
  <object-name>BugradGelenkOben</object-name>
<property>gear/gear/compression-norm</property>
<interpolation>
            <entry>
                <ind>0.73</ind>
                <dep>-50</dep>
            </entry>
            <entry>
                <ind>0.3</ind>
                <dep>0.0</dep>
            </entry>
        </interpolation>
  <axis>
  <x1-m>-1.70422</x1-m>
  <y1-m>1</y1-m>
  <z1-m>-1.19225</z1-m>
  <x2-m>-1.70422</x2-m>
  <y2-m>-1</y2-m>
  <z2-m>-1.19225</z2-m>
  </axis>
</animation> 
<animation>
  <type>rotate</type>
  <object-name>BugradGelenkUnten</object-name>
<property>gear/gear/compression-norm</property>
<interpolation>
            <entry>
                <ind>0.73</ind>
                <dep>-50</dep>
            </entry>
            <entry>
                <ind>0.3</ind>
                <dep>0.0</dep>
            </entry>
        </interpolation>
  <axis>
  <x1-m>-1.74406</x1-m>
  <y1-m>-1</y1-m>
  <z1-m>-1.40423</z1-m>
  <x2-m>-1.74406</x2-m>
  <y2-m>1</y2-m>
  <z2-m>-1.40423</z2-m>
  </axis>
</animation>
 <!-- drehende Raeder -->
<animation>
  <type>spin</type>
  <object-name>Bugrad</object-name>
  <condition>
    <equals>
      <property>gear/gear[0]/position-norm</property>
      <value>1</value>
    </equals>
  </condition>
  <property>gear/gear[0]/rollspeed-ms</property>
  <factor>-20</factor>
  <center>
    <x-m>-1.73131</x-m>
    <y-m>0</y-m>
    <z-m>-1.65133</z-m>
  </center>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
 </animation>
 
