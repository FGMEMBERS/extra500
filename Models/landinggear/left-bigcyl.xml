<PropertyList>

<path>left-bigcyl.ac</path>

<!-- submodels -->


<!-- cylinder movement --> 
 <animation>
  <type>translate</type>
  <object-name>Cylinder</object-name>
  <property>gear/gear[1]/position-norm</property>
  <interpolation>
   <entry><ind>0</ind><dep>0.430</dep></entry>
   <entry><ind>0.25</ind><dep>0.415</dep></entry>
   <entry><ind>0.5</ind><dep>0.3</dep></entry>
   <entry><ind>1</ind><dep>0</dep></entry>
  </interpolation>  
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 
</PropertyList>
