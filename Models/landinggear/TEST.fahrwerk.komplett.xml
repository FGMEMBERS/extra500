<PropertyList>

<path>test-lg.ac</path>

<!-- submodels -->

<model>
  <name>LLG</name>
  <path>Aircraft/extra500/Models/landinggear/left-lg.xml</path>	
</model>

<model>
  <name>RLG</name>
  <path>Aircraft/extra500/Models/landinggear/right-lg.xml</path>	
</model>

<model>
  <name>LG-doormech</name>
  <path>Aircraft/extra500/Models/landinggear/left-doormech.xml</path>	
	<offsets>
		<x-m>-0.02</x-m>
		<y-m>-0.52013</y-m>	
		<z-m>-0.90911</z-m>
	</offsets>
</model>

<model>
  <name>RG-doormech</name>
  <path>Aircraft/extra500/Models/landinggear/right-doormech.xml</path>	
	<offsets>
		<x-m>-0.02</x-m>
		<y-m>0.52013</y-m>	
		<z-m>-0.90911</z-m>
		<heading-deg>180</heading-deg>
	</offsets>
</model>

<model>
  <name>testcube</name>
  <path>Aircraft/extra500/Models/landinggear/testcube.ac</path>	
	<offsets>
		<x-m>0.02</x-m>
		<y-m>-0.14</y-m>	
		<z-m>-0.99</z-m>
	</offsets>
</model>

<animation>
  <name>BugFahrwerkKomplett</name>
  <object-name>Bugrad_Daempfer</object-name>
  <object-name>BugradGabel</object-name>
  <object-name>Bugrad</object-name>
  <object-name>BugradGelenkUnten</object-name>
  <object-name>BugradGelenkOben</object-name>
 </animation>
  
<!-- BUGFAHRWERK --> 

<!-- Bugradeinzug  -->
<animation>
  <type>rotate</type>
  <object-name>BugFahrwerkKomplett</object-name>
<property>gear/gear[0]/position-norm</property>
  <interpolation>
            <entry>
                <ind>0.0</ind>
                <dep>105</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>0</dep>
            </entry>
        </interpolation>
  <axis>
    <x1-m>-1.60481</x1-m>
    <y1-m>1.0</y1-m>
    <z1-m>-0.98046</z1-m>
	<x2-m>-1.60481</x2-m>
    <y2-m>-1.0</y2-m>
    <z2-m>-0.98046</z2-m>
  </axis>
  </animation> 
  
<!-- lenkung Bugfahrwerk-->  
<animation>
  <type>rotate</type>
  <object-name>BugFahrwerkKomplett</object-name>
  <condition>
    <equals>
      <property>gear/gear[0]/position-norm</property>
      <value>1</value>
    </equals>
  </condition>
  <property>surface-positions/rudder-pos-norm</property>
  <interpolation>
   <entry><ind>-1</ind><dep>-30</dep></entry>
   <entry><ind> 0</ind><dep>  0</dep></entry>
   <entry><ind> 1</ind><dep> 30</dep></entry>
  </interpolation>   
  <axis>
    <x1-m>-1.63609</x1-m>
    <y1-m>0</y1-m>
    <z1-m>-1.14638</z1-m>
    <x2-m>-1.69078</x2-m>
    <y2-m>0</y2-m>
    <z2-m>-1.43637</z2-m>
  </axis>
</animation>
  
<!-- Bugfahrwerksdaempfung -->
<animation>
  <type>translate</type>
  <object-name>BugradGabel</object-name>
  <object-name>BugradGelenkUnten</object-name>
  <object-name>Bugrad</object-name>
<property>gear/gear/compression-norm</property>
<interpolation>
            <entry>
                <ind>0.73</ind>
                <dep>-0.12</dep>
            </entry>
            <entry>
                <ind>0.3</ind>
                <dep>0.0</dep>
            </entry>
        </interpolation>
  <axis>
  <x1-m>-1.63609</x1-m>
  <y1-m>0</y1-m>
  <z1-m>-1.14638</z1-m>
  <x2-m>-1.69078</x2-m>
  <y2-m>0</y2-m>
  <z2-m>-1.43637</z2-m>
  </axis>
</animation> 
<animation>
  <type>rotate</type>
  <object-name>BugradGelenkOben</object-name>
<property>gear/gear/compression-norm</property>
<interpolation>
            <entry>
                <ind>0.73</ind>
                <dep>-50</dep>
            </entry>
            <entry>
                <ind>0.3</ind>
                <dep>0.0</dep>
            </entry>
        </interpolation>
  <axis>
  <x1-m>-1.70422</x1-m>
  <y1-m>1</y1-m>
  <z1-m>-1.19225</z1-m>
  <x2-m>-1.70422</x2-m>
  <y2-m>-1</y2-m>
  <z2-m>-1.19225</z2-m>
  </axis>
</animation> 
<animation>
  <type>rotate</type>
  <object-name>BugradGelenkUnten</object-name>
<property>gear/gear/compression-norm</property>
<interpolation>
            <entry>
                <ind>0.73</ind>
                <dep>-50</dep>
            </entry>
            <entry>
                <ind>0.3</ind>
                <dep>0.0</dep>
            </entry>
        </interpolation>
  <axis>
  <x1-m>-1.74406</x1-m>
  <y1-m>-1</y1-m>
  <z1-m>-1.40423</z1-m>
  <x2-m>-1.74406</x2-m>
  <y2-m>1</y2-m>
  <z2-m>-1.40423</z2-m>
  </axis>
</animation>
 <!-- drehende Raeder -->
<animation>
  <type>spin</type>
  <object-name>Bugrad</object-name>
  <condition>
    <equals>
      <property>gear/gear[0]/position-norm</property>
      <value>1</value>
    </equals>
  </condition>
  <!--<property>gear/gear[0]/rollspeed-ms</property>-->
  <property>sim/multiplay/generic/float[11]</property>
  <factor>-20</factor>
  <center>
    <x-m>-1.73131</x-m>
    <y-m>0</y-m>
    <z-m>-1.65133</z-m>
  </center>
  <axis>
    <x>0</x>
    <y>1</y>
    <z>0</z>
  </axis>
 </animation>
 
<!-- MAIN GEAR LEFT-->

<animation>
  <type>rotate</type>
  <object-name>LLG</object-name>
 <property>gear/gear[1]/position-norm</property> 
  <interpolation>
            <entry>
                <ind>0</ind>
                <dep>-92.5</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>0</dep>
            </entry>
        </interpolation>
  <axis>
  <x1-m>0.93407</x1-m>
  <y1-m>-0.18040</y1-m>
  <z1-m>-1.1038</z1-m>
  <x2-m>0.93407</x2-m>
  <y2-m>-0.41606</y2-m>
  <z2-m>-0.98523</z2-m>
  </axis>
</animation>

<animation>
  <type>rotate</type>
  <object-name>LG-doormech</object-name>
<property>/systems/gear/Lupperdoor-pos</property>
  <interpolation>
            <entry>
                <ind>0.0</ind>
                <dep>130</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>45</dep>
            </entry>
        </interpolation>
  <axis>
    <x1-m>0</x1-m>
    <y1-m>-0.52013</y1-m>
    <z1-m>-0.90911</z1-m>
	<x2-m>1</x2-m>
    <y2-m>-0.52013</y2-m>
    <z2-m>-0.90911</z2-m>
  </axis>
  </animation> 

<!-- FAHRWERK RECHTS-->
<animation>
  <type>rotate</type>
  <object-name>RLG</object-name>
 <property>gear/gear[2]/position-norm</property> 
  <interpolation>
            <entry>
                <ind>0</ind>
                <dep>92.5</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>0</dep>
            </entry>
        </interpolation>
  <axis>
  <x1-m>0.93407</x1-m>
  <y1-m>0.18040</y1-m>
  <z1-m>-1.1038</z1-m>
  <x2-m>0.93407</x2-m>
  <y2-m>0.41606</y2-m>
  <z2-m>-0.98523</z2-m>
  </axis>
</animation>

<animation>
  <type>rotate</type>
  <object-name>RG-doormech</object-name>
<property>/systems/gear/Rupperdoor-pos</property>
  <interpolation>
            <entry>
                <ind>0.0</ind>
                <dep>-130</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>-45</dep>
            </entry>
        </interpolation>
  <axis>
    <x1-m>0</x1-m>
    <y1-m>0.52013</y1-m>
    <z1-m>-0.90911</z1-m>
	<x2-m>1</x2-m>
    <y2-m>0.52013</y2-m>
    <z2-m>-0.90911</z2-m>
  </axis>
  </animation> 

 
</PropertyList>
