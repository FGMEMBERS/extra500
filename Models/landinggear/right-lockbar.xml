<PropertyList>

<path>left-lockbar.ac</path>

<!-- submodels -->



<!-- lockstrut rotation --> 
 <animation>
  <type>translate</type>
  <object-name>cylinder</object-name>
  <object-name>swivel</object-name>
  <property>gear/gear[2]/position-norm</property>
  <interpolation>
   <entry><ind>0</ind><dep>0.030</dep></entry>
   <entry><ind>1</ind><dep>0</dep></entry>
  </interpolation>  
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>
 
</PropertyList>
