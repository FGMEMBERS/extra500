<PropertyList>

<path>right-middle-strut.ac</path>

<!-- submodels -->

<model>
  <name>right-wheelass</name>
  <path>Aircraft/extra500/Models/landinggear/right-wheelass.xml</path>
	<offsets>
		<x-m>0.0</x-m>
		<y-m>0.27550</y-m>	
		<z-m>-0.24013</z-m>
	</offsets>
</model>

<model>
  <name>right-damper-upper</name>
  <path>Aircraft/extra500/Models/landinggear/right-damper-upper.xml</path>
	<offsets>
		<x-m>0.0</x-m>
		<y-m>0</y-m>	
		<z-m>0</z-m>
		<heading-deg>180</heading-deg>
	</offsets>
</model>

<model>
  <name>right-damper-lower</name>
  <path>Aircraft/extra500/Models/landinggear/left-damper-lower.ac</path>
	<offsets>
		<x-m>0.0</x-m>
		<y-m>0</y-m>	
		<z-m>-0.22100</z-m>
		<heading-deg>180</heading-deg>
	</offsets>
</model>

<model>
  <name>right-guiderod-lower</name>
  <path>Aircraft/extra500/Models/landinggear/left-guiderod.lower.ac</path>
	<offsets>
		<x-m>0.06487</x-m>
		<y-m>-0.01291</y-m>	
		<z-m>-0.13116</z-m>
	</offsets>
</model>

 <animation>
  <type>rotate</type>
  <object-name>blockarm.right</object-name>
  <object-name>block.in.right</object-name>
  <object-name>block.out.right</object-name>
  <object-name>right-guiderod-lower</object-name>
  <property>gear/gear[2]/position-norm</property>
  <interpolation>
            <entry>
                <ind>0.0</ind>
                <dep>49</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>-35</dep>
            </entry>
        </interpolation>
  <axis>
	<x1-m>0</x1-m>
    <y1-m>0</y1-m>
    <z1-m>0</z1-m>
    <x2-m>0</x2-m>
    <y2-m>0</y2-m>
    <z2-m>1</z2-m>
  </axis>
  </animation>

 <animation>
  <type>rotate</type>
  <object-name>block.in.right</object-name>
  <property>gear/gear[2]/position-norm</property>
  <interpolation>
            <entry>
                <ind>0.0</ind>
                <dep>-49</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>35</dep>
            </entry>
        </interpolation>
  <axis>
	<x1-m>0.03233</x1-m>
    <y1-m>-0.01597</y1-m>
    <z1-m>0</z1-m>
    <x2-m>0.03233</x2-m>
    <y2-m>-0.01597</y2-m>
    <z2-m>1</z2-m>
  </axis>
  </animation>

 <animation>
  <type>rotate</type>
  <object-name>block.out.right</object-name>
  <object-name>right-guiderod-lower</object-name>
  <property>gear/gear[2]/position-norm</property>
  <interpolation>
            <entry>
                <ind>0.0</ind>
                <dep>-47</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>34</dep>
            </entry>
        </interpolation>
  <axis>
	<x1-m>0.06487</x1-m>
    <y1-m>-0.01291</y1-m>
    <z1-m>0</z1-m>
    <x2-m>0.06487</x2-m>
    <y2-m>-0.01291</y2-m>
    <z2-m>1</z2-m>
  </axis>
  </animation>

 <animation>
  <type>rotate</type>
  <object-name>right-guiderod-lower</object-name>
  <property>gear/gear[2]/position-norm</property>
  <interpolation>
            <entry>
                <ind>0.0</ind>
                <dep>140</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>140</dep>
            </entry>
        </interpolation>
  <axis>
	<x1-m>0</x1-m>
    <y1-m>-0.01291</y1-m>
    <z1-m>-0.13116</z1-m>
    <x2-m>1</x2-m>
    <y2-m>-0.01291</y2-m>
    <z2-m>-0.13116</z2-m>
  </axis>
  </animation>

 <animation>
  <type>rotate</type>
  <object-name>right-guiderod-lower</object-name>
  <property>gear/gear[2]/compression-norm</property>
  <interpolation>
            <entry>
                <ind>0.0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>42</dep>
            </entry>
        </interpolation>
  <axis>
	<x1-m>0</x1-m>
    <y1-m>0.01291</y1-m>
    <z1-m>-0.13116</z1-m>
    <x2-m>1</x2-m>
    <y2-m>0.01291</y2-m>
    <z2-m>-0.13116</z2-m>
  </axis>
  </animation>



<!-- upperstrut rotation --> 
 <animation>
  <type>rotate</type>
  <object-name>right-damper-upper</object-name>
  <property>gear/gear[2]/compression-norm</property>
  <interpolation>
            <entry>
                <ind>0.0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>50</dep>
            </entry>
        </interpolation>
  <axis>
	<x1-m>0.0</x1-m>
    <y1-m>-0.0</y1-m>
    <z1-m>0</z1-m>
    <x2-m>1.0</x2-m>
    <y2-m>0.0</y2-m>
    <z2-m>0</z2-m>
  </axis>
  </animation>

<!-- lower strut rotation --> 
 <animation>
  <type>rotate</type>
  <object-name>right-damper-lower</object-name>
  <property>gear/gear[2]/compression-norm</property>
  <interpolation>
            <entry>
                <ind>0.0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>50</dep>
            </entry>
        </interpolation>
  <axis>
	<x1-m>0.0</x1-m>
    <y1-m>-0.0</y1-m>
    <z1-m>-0.22100</z1-m>
    <x2-m>1.0</x2-m>
    <y2-m>0.0</y2-m>
    <z2-m>-0.22100</z2-m>
  </axis>
  </animation>

<animation>
  <type>translate</type>
  <object-name>right-wheelass</object-name>
  <property>gear/gear[2]/compression-norm</property>
  <factor>0.3048</factor>
  <axis>
  	<x>0</x>
  	<y>0.7</y>
  	<z>1</z>
  </axis>
</animation> 


</PropertyList>
