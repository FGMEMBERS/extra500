<PropertyList>

<path>nlg-torquelink.ac</path>

<!-- submodels -->
  
<!-- dampening movement-->  
<animation>
  <type>rotate</type>
  <object-name>nlg-torque.link.lower</object-name>
  <property>gear/gear/compression-norm</property>
  	<interpolation>
            <entry>
                <ind>1</ind>
                <dep>-180</dep>
            </entry>
            <entry>
                <ind>0.0</ind>
                <dep>0.0</dep>
            </entry>
        </interpolation>   
  <axis>
    <x1-m>-0.09753</x1-m>
    <y1-m>0</y1-m>
    <z1-m>-0.348073</z1-m>
    <x2-m>-0.09753</x2-m>
    <y2-m>1</y2-m>
    <z2-m>-0.34807</z2-m>
  </axis>
</animation>
  

 

</PropertyList>
