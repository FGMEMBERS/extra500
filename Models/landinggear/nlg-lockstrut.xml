<PropertyList>

<path>nlg-lockstrut.ac</path>

<!-- submodels -->


<!-- LOCKSTRUT RELATIVE TO NLG-->  
<animation>
  <type>rotate</type>
  <object-name>vstrut</object-name>
  <property>gear/gear[0]/position-norm</property>
  <interpolation>
   <entry><ind>0</ind><dep>123</dep></entry>
   <entry><ind>0.25</ind><dep>140</dep></entry>
   <entry><ind>0.5</ind><dep>122</dep></entry>
   <entry><ind>0.75</ind><dep>84</dep></entry>
   <entry><ind>1</ind><dep>0</dep></entry>
  </interpolation>   
  <axis>
    <x1-m>0.</x1-m>
    <y1-m>-0.142</y1-m>
    <z1-m>0</z1-m>
    <x2-m>1</x2-m>
    <y2-m>-0.142</y2-m>
    <z2-m>0</z2-m>
  </axis>
</animation>
  
</PropertyList>
