<PropertyList>

<path>left-damper-upper.ac</path>

<!-- submodels -->

<model>
  <name>left-damper</name>
  <path>Aircraft/extra500/Models/landinggear/left-damper.xml</path>
	<offsets>
		<x-m>0.0</x-m>
		<y-m>-0.03896</y-m>	
		<z-m>-0.08753</z-m>
	</offsets>
</model>

<!-- damper rotation --> 
 <animation>
  <type>rotate</type>
  <object-name>left-damper</object-name>
  <property>gear/gear[1]/compression-norm</property>
  <interpolation>
            <entry>
                <ind>0.0</ind>
                <dep>54.4</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>80</dep>
            </entry>
        </interpolation>
  <axis>
	<x1-m>0.0</x1-m>
    <y1-m>-0.03896</y1-m>
    <z1-m>-0.08753</z1-m>
    <x2-m>1.0</x2-m>
    <y2-m>-0.03896</y2-m>
    <z2-m>-0.08753</z2-m>
  </axis>
  </animation>

</PropertyList>
