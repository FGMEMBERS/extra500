<PropertyList>

<path>extra500_EngineInstruments.ac</path>

<animation>
  <type>rotate</type>
  <object-name>Needle.TRQ</object-name>
  <property>/fdm/jsbsim/aircraft/engine/TRQ-perc</property>
  <interpolation>
            <entry>
                <ind>0</ind>
                <dep>-124</dep>
            </entry>
            <entry>
                <ind>125</ind>
                <dep>124</dep>
            </entry>
        </interpolation>
  <axis>
  <x1-m>1</x1-m>
  <y1-m>-0.02383</y1-m>
  <z1-m>0.07613</z1-m>
  <x2-m>-1</x2-m>
  <y2-m>-0.02383</y2-m>
  <z2-m>0.07613</z2-m>
  </axis>
</animation>


</PropertyList>

