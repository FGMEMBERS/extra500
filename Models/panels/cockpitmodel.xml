<!-- ConditionLever  
/controls/engines/engine[0]/condition-lever-norm is controlled by: 	
		/extra500/engine/cutoff (press: s)
		/controls/engines/engine[0]/propeller-pitch (press: n/N)
in Systems/extra500-system-indication.xml-->
<animation>
  <type>rotate</type>
  <object-name>ConditionLever</object-name>
  <property>/controls/engines/engine[0]/condition-lever-norm</property>
  <interpolation>
            <entry>
                <ind>0</ind>
                <dep>22.7</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>-18.2</dep>
            </entry>
        </interpolation>
  <axis>
    <x1-m>-0.6579</x1-m>
    <y1-m>-1</y1-m>
    <z1-m>-0.4872</z1-m>
    <x2-m>-0.6579</x2-m>
    <y2-m>1</y2-m>
    <z2-m>-0.4872</z2-m>
  </axis>
</animation>

<animation>
  <type>translate</type>
  <object-name>ConditionLever</object-name>
  <property>/controls/engines/engine[0]/condition-lever-norm</property>
  <interpolation>
            <entry>
                <ind>0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0.01</ind>
                <dep>0.02</dep>
            </entry>
            <entry>
                <ind>0.48</ind>
                <dep>0.02</dep>
            </entry>
            <entry>
                <ind>0.49</ind>
                <dep>0.0</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>0.0</dep>
            </entry>
  </interpolation>
  <axis>
    <x1-m>-0.6579</x1-m>
    <y1-m>0</y1-m>
    <z1-m>-0.4872</z1-m>
    <x2-m>-0.61302</x2-m>
    <y2-m>0</y2-m>
    <z2-m>-0.34039</z2-m>
  </axis>
</animation>

<animation>
