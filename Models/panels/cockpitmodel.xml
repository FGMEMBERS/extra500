<!-- PowerLever -->
<model>
  <name>gear warn mute switch</name>
  <path>Aircraft/extra500/Models/interior/gearwarnmute.xml</path>
	<offsets>
		<x-m>-0.61302</x-m>
		<y-m>-0.04777</y-m>	
		<z-m>-0.34039</z-m>
		<roll-deg>90</roll-deg>
		<pitch-deg>0</pitch-deg>
		<yaw-deg>0</yaw-deg>
	</offsets>
</model>

<animation>
  <type>rotate</type>
  <object-name>PowerLever</object-name>
  <object-name>gear warn mute switch</object-name>
  <property>controls/engines/engine/throttle</property>
  <condition>
    <equals>
      <property>controls/engines/engine/reverser</property>
      <value>1</value>
    </equals>
  </condition>
  <interpolation>
            <entry>
                <ind>0</ind>
                <dep>-5.4</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>-41.7</dep>
            </entry>
        </interpolation>
  <axis>
    <x1-m>-0.6579</x1-m>
    <y1-m>1</y1-m>
    <z1-m>-0.4872</z1-m>
    <x2-m>-0.6579</x2-m>
    <y2-m>-1</y2-m>
    <z2-m>-0.4872</z2-m>
  </axis>
</animation>
<animation>
  <type>rotate</type>
  <object-name>PowerLever</object-name>
  <object-name>gear warn mute switch</object-name>
  <property>controls/engines/engine/throttle</property>
  <condition>
    <equals>
      <property>controls/engines/engine/reverser</property>
      <value>0</value>
    </equals>
  </condition>
  <interpolation>
            <entry>
                <ind>0</ind>
                <dep>-5.4</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>40.5</dep>
            </entry>
        </interpolation>
  <axis>
    <x1-m>-0.6579</x1-m>
    <y1-m>1</y1-m>
    <z1-m>-0.4872</z1-m>
    <x2-m>-0.6579</x2-m>
    <y2-m>-1</y2-m>
    <z2-m>-0.4872</z2-m>
  </axis>
</animation>

<animation>
	<type>pick</type>
	<object-name>PowerLever</object-name>
	<action>
		<button>0</button>
		<repeatable>false</repeatable>
		<binding>
			<command>nasal</command>
			<script>
				<![CDATA[
					UI.click("Engine reverser");
				]]>
			</script>
		</binding>
	</action>
  <action>
    <button>3</button>
    <repeatable>false</repeatable>
    <binding>
