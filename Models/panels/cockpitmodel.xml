      <script><![CDATA[
		setprop("/controls/engines/engine/throttle",getprop("/controls/engines/engine/throttle")+.0025);
	]]></script>
    </binding>
  </action>
  <action>
    <button>4</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script><![CDATA[
		setprop("/controls/engines/engine/throttle",getprop("/controls/engines/engine/throttle")-.0025);
	]]></script>
    </binding>
  </action>
</animation>

