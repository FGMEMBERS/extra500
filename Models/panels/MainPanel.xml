<PropertyList>

<path>MainPanel.ac</path>

  <animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-m>50</max-m>
  </animation>

<effect>
    <name>MainPanel</name>
    <inherits-from>Models/Effects/panel</inherits-from>
    <object-name>MainPanel</object-name>
    <object-name>Glareshield.lower</object-name>
    <parameters>
		<grain-magnification type="float">2000</grain-magnification>
    </parameters>
</effect>
<effect>
    <name>AP</name>
    <inherits-from>Models/Effects/panel</inherits-from>
    <object-name>autopilot.case</object-name>
    <parameters>
		<grain-magnification type="float">4000</grain-magnification>
    </parameters>
</effect>
<effect>
    <name>yokecover</name>
    <inherits-from>Models/Effects/panel</inherits-from>
    <object-name>YokeFrameR</object-name>
    <object-name>YokeFrameL</object-name>
    <parameters>
		<grain-magnification type="float">200</grain-magnification>
    </parameters>
</effect>

  <!--Submodels--> 

  <animation>
	  <object-name>yokes</object-name>
	  <type>select</type>
	  <condition>
		  <equals>
			  <property>/extra500/yokes/visible</property>
			  <value>1</value>
		  </equals>
	  </condition>
  </animation> 
	
  <animation>
	  <type>material</type>
	  <object-name>yokes</object-name>
	  <transparency>
		  <alpha-prop>/extra500/yokes/transparency</alpha-prop>
		  <factor>1</factor>
	  </transparency>
  </animation>
  
  <effect>
	  <inherits-from>Effects/model-transparent</inherits-from>
	  <object-name>yokes</object-name>
  </effect>

<!-- Submodels -->

