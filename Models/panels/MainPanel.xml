<PropertyList>

<path>MainPanel.ac</path>

  <animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-m>50</max-m>
  </animation>

  <animation>
  	<object-name>yokes</object-name>
  	<type>select</type>
  	<condition>
   		<equals>
    			<property>/extra500/yokes/visible</property>
    			<value>1</value>
   		</equals>
  	</condition>
  </animation>

