<!-- glarelights -->

<animation>
        <type>material</type>
        <object-name>glare.face.middle</object-name>
        <object-name>glare.face.outer</object-name>
	  <emission>
	    	<red>1</red>
            <green>1</green>
	    	<blue>1</blue>
	    	<factor-prop>/extra500/light/glare/state</factor-prop>
        </emission>
</animation>

      <animation>
            <type>light</type>
            <light-type>point</light-type>
            <name>LightSrc200</name>
            <object-name>glare.light</object-name>
            <nopreview/>
            <position>
			<x>-0.2</x>
			<y>0.0</y>	
			<z>0.0</z>
            </position>
            <ambient>
                  <r>0.9</r>
                  <g>0.9</g>
                  <b>0.9</b>
                  <a>1.0</a>
            </ambient>
            <diffuse>
                  <r>0.9</r>
                  <g>0.9</g>
                  <b>0.9</b>
                  <a>1.0</a>
            </diffuse>
            <specular>
                  <r>0.9</r>
                  <g>0.9</g>
                  <b>0.9</b>
                  <a>1.0</a>
            </specular>
		<dim-factor>
      		<property>/extra500/light/glare/state</property>
   		</dim-factor>
            <attenuation>
                  <c>5.0</c>
                  <l>0.2</l>
                  <q>0.005</q>
            </attenuation>
            <near-m>0.1</near-m>
            <far-m>5</far-m>
      </animation>

      <animation>
            <type>select</type>
            <object-name>LightSrc200</object-name>
            <condition>
                  <and>
                        <property>/sim/rendering/rembrandt/enabled</property>
                        <greater-than>
                              <property>/sim/rendering/shaders/lights</property>
                              <value>0</value>
                        </greater-than>
           			<greater-than>
            			<property>/extra500/light/glare/state</property>
                  		<value>0</value>
            		</greater-than>
                  </and>
            </condition>
      </animation>

