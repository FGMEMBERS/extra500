<PropertyList>
	
	<path>wing.ice.ac</path>
	
	
	<!-- wing front ice -->
	<animation>
		<type>select</type>
		<object-name>wing.ICE.front</object-name>
		<condition>
			<greater-than>
				<property>/environment/aircraft-effects/frost-level-front</property>
				<value>0</value>
			</greater-than>
		</condition>
	</animation>
	<animation>
		<type>translate</type>
		<object-name>wing.ICE.front</object-name>
		<property>/environment/aircraft-effects/frost-level-front</property>
		<factor>0.05</factor>
		<offset-m>0.10</offset-m>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	<effect>
		<inherits-from>Aircraft/extra500/Models/Effects/extra500-ice</inherits-from>
		<object-name>wing.ICE.front</object-name>
		<parameters>
			<frost-level><use>/environment/aircraft-effects/frost-level-front</use></frost-level>
		</parameters>
	</effect>
	
	
	
