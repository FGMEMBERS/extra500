<!-- wing LH pitot ice -->
	<!-- pitot none heated part -->
	<animation>
		<object-name>wing.ICE.LH.pitot.front</object-name>
		<type>select</type>
		<condition>
			<greater-than>
				<property>/environment/aircraft-effects/frost-level-front</property>
				<value>0</value>
			</greater-than>
		</condition>
	</animation>
	<animation>
		<type>scale</type>
		<object-name>wing.ICE.LH.pitot.front</object-name>
		<property>/environment/aircraft-effects/frost-level-front</property>
		<x-min>0.1</x-min>
		<y-min>0.1</y-min>
		<z-min>0.1</z-min>
		<x-offset>0.2</x-offset>
		<y-offset>0.2</y-offset>
		<z-offset>0.8</z-offset>
		
		<x-factor>0.8</x-factor>
		<y-factor>0.8</y-factor>
		<z-factor>0.2</z-factor>
		
		<x-max>1.0</x-max>
		<y-max>1.0</y-max>
		<z-max>1.0</z-max>
		<center>
			<x-m> 0.35088  </x-m>
			<y-m> -4.23622 </y-m>
			<z-m> -0.26685  </z-m>
		</center><!---->
	</animation>
	<effect>
		<inherits-from>Aircraft/extra500/Models/Effects/extra500-ice</inherits-from>
		<object-name>wing.ICE.LH.pitot.front</object-name>
		<parameters>
			<frost-level><use>/environment/aircraft-effects/frost-level-front</use></frost-level>
		</parameters>
	</effect>
	
