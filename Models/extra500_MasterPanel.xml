<PropertyList>

<path>extra500_MasterPanel.ac</path>

<!-- MasterPanel.Autopilot.Master -->
<animation>
  <type>rotate</type>
  <object-name>MasterPanel.Autopilot.Master</object-name>
  <property>extra500/MasterPanel/Autopilot/Master/state</property>
  <factor>-25</factor>
  <axis>
    <x1-m>0.01</x1-m>
    <y1-m>-1</y1-m>
    <z1-m>-0.002246</z1-m>
    <x2-m>0.01</x2-m>
    <y2-m>1</y2-m>
    <z2-m>-0.002246</z2-m>
  </axis>		
</animation>
<animation>
  <type>pick</type>
  <object-name>MasterPanel.Autopilot.Master</object-name>
  <object-name>MasterPanel.Autopilot.Master.Ring</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script><![CDATA[
		UI.click("Autopilot Master >");
	]]></script>
    </binding>
  </action>
  <action>
    <button>1</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script><![CDATA[
		UI.click("Autopilot Master <");
	]]></script>
    </binding>
  </action>
  <action>
    <button>3</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script><![CDATA[
		UI.click("Autopilot Master >");
	]]></script>
    </binding>
  </action>
  <action>
    <button>4</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script><![CDATA[
		UI.click("Autopilot Master <");
	]]></script>
    </binding>
  </action>
</animation>
<animation>
	<type>material</type>
	<object-name>MasterPanel.Autopilot.Master.Ring</object-name>
	<condition>
		<equals>
		<property>extra500/MasterPanel/Autopilot/Master/state</property>
		<value>1</value>
		</equals>
	</condition>
	<emission>	
		<red>0.7</red>	
		<green>0.5</green>	
		<blue>0.0</blue>
	</emission>
</animation>
<animation>
	<type>material</type>
	<object-name>MasterPanel.Autopilot.Master.Ring</object-name>
	<condition>
		<equals>
		<property>extra500/MasterPanel/Autopilot/Master/state</property>
		<value>-1</value>
		</equals>
	</condition>
	<emission>	
		<red>0.0</red>	
		<green>0.5</green>	
		<blue>0.0</blue>
	</emission>
</animation>
