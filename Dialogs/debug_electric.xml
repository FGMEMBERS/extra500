<?xml version="1.0"?>

<PropertyList>
  <name>debug_electric</name>
  <modal>false</modal>
  <layout>vbox</layout>
  <resizable>true</resizable>
  <x>-20</x>
  <y>20</y>

  <group>
    <layout>hbox</layout>

    <button>
      <legend>-</legend>
      <pref-width>16</pref-width>
      <pref-height>16</pref-height>
      <border>2</border>
      <binding>
       <command>dialog-show</command>
       <dialog-name>chat</dialog-name>
      </binding>
      <binding>
        <command>dialog-close</command>
      </binding>
    </button>

    <empty><stretch>1</stretch></empty>

    <text>
      <label>Electric system Flow</label>
    </text>

    <empty><stretch>1</stretch></empty>

    <button>
      <legend></legend>
      <pref-width>16</pref-width>
      <pref-height>16</pref-height>
      <border>2</border>
      <binding>
        <command>dialog-close</command>
      </binding>
    </button>
  </group>

  <hrule/>

       <textbox>
	       <font>
		<name>FIXED_8x13</name>
		</font>
	
	       <halign>fill</halign>
	       <valign>fill</valign>
	       <stretch>true</stretch>
	       <pref-width>1200</pref-width>
	       <pref-height>800</pref-height>
	       <padding>5</padding>
	       <slider>20</slider>
	       <live>true</live>
	       <wrap>true</wrap>
	       <editable>false</editable>
	       <top-line>-1</top-line>
	       <property>/extra500/DebugOutput</property>
       </textbox>

       <!---->
       <group>
	       <layout>hbox</layout>
	       <padding>4</padding>
	       <text>
		       <label>Name</label>
	       </text>
	       <input>
		       <name>composeName</name>
		       <stretch>true</stretch>
		       <halign>fill</halign>
		       <stretch>true</stretch>
		       <length>50</length>
		       <property>/extra500/debug/electric/name</property>
	       </input>
	       <text>
		       <label>excludeType</label>
	       </text>
	       <input>
		       <name>excludeType</name>
		       <stretch>true</stretch>
		       <halign>fill</halign>
		       <stretch>true</stretch>
		       <length>50</length>
		       <property>/extra500/debug/electric/excludeType</property>
	       </input>
	       
	       <button>
		       <legend>apply</legend>
		       <default>true</default>
		       <binding>
			       <command>dialog-apply</command>
			       <object-name>excludeType</object-name>
		       </binding>
		       <binding>
			       <command>dialog-apply</command>
			       <object-name>composeName</object-name>
		       </binding>
		       <binding>
			       <command>nasal</command>
			       <script>
				       Part.etd.debugName = getprop("/extra500/debug/electric/name");
				       Part.etd.excludeType = getprop("/extra500/debug/electric/excludeType");
				       
			       </script>
		       </binding>
	       </button>
  </group>
</PropertyList>
